from collections import Counter
class Solution:
    def findShortestSubArray(self, nums: List[int]) -> int:
        ans=0
        l=[]
        d=sorted(Counter(nums).items(),key=lambda item:item[1],reverse=True)
        ans=d[0][1]
        for i,j in d:
            if ans > j:break
            l.append(abs(nums.index(i)-(len(nums)-1-nums[::-1].index(i)))+1)
            
        return min(l)
           
            
        
